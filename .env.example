# Sales Prep Agent Configuration
# Copy this file to .env and fill in values

# Toggle mock data (set to "false" for live Azure AI Search + Graph)
USE_MOCK_DATA=true

# ── Foundry IQ — Azure AI Search ────────────────────────────────────────
# AZURE_SEARCH_ENDPOINT=https://<name>.search.windows.net
# AZURE_SEARCH_KEY=<query-key-for-runtime>
# AZURE_SEARCH_ADMIN_KEY=<admin-key-for-seeding-only>
# AZURE_SEARCH_INDEX=sales-enablement

# ── Work IQ — Microsoft Graph ─────────────────────────────────────────
# GRAPH_TENANT_ID=<entra-tenant-id>
# GRAPH_USER_ID=<user-principal-name-or-object-id>
#
# Option A: Entra Agent ID (preview) — preferred
# GRAPH_BLUEPRINT_CLIENT_ID=<blueprint-app-id>
# GRAPH_BLUEPRINT_SECRET=<blueprint-client-secret>
# GRAPH_AGENT_CLIENT_ID=<agent-identity-app-id>
#
# Option B: Legacy app registration (fallback)
# GRAPH_CLIENT_ID=<app-registration-client-id>
# GRAPH_CLIENT_SECRET=<client-secret-value>

# ── Legacy / MCP-based (optional) ───────────────────────────────────────
# WORKIQ_TENANT_ID=<your-entra-tenant-id>
# FABRIC_WORKSPACE_ID=<your-fabric-workspace-id>

# Output directory for generated documents
OUTPUT_DIR=output
